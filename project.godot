config_version=4

[application]

config/name="'Tis But A Scratch"
run/main_scene="res://intro.tscn"
boot_splash/image="res://assets/main/intro/introhex.png"
boot_splash/bg_color=Color( 0.0823529, 0, 0.0862745, 1 )
config/icon="res://assets/logo.png"
config/windows_native_icon="res://logo.ico"

[autoload]

GlobalVars="*res://global_vars.gd"

[display]

window/size/width=1920
window/size/height=1080
window/size/fullscreen=true
window/size/test_width=1600
window/size/test_height=900
window/dpi/allow_hidpi=true
window/stretch/mode="2d"
window/stretch/aspect="keep"

[gui]

common/drop_mouse_on_gui_input_disabled=true
theme/use_hidpi=true

